{
  "extends": ["next/core-web-vitals", "prettier"],
  "root": true,
  "rules": {
    "simple-import-sort/imports": "warn",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error"],
    "react/no-unescaped-entities": 0
